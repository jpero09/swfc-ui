{
    "preset": "google",
    "verbose": false,
    "maximumLineLength": 120,
    "disallowSpaceAfterKeywords": [
      "if",
      "for",
      "while",
      "do",
      "switch",
      "catch"
    ],
    "requireSpaceAfterKeywords": [
      "else",
      "return",
      "try",
      "catch"
    ],  
    "disallowKeywordsOnNewLine": null,
    "requireKeywordsOnNewLine": [
      "else",
      "catch"
    ],
    "excludeFiles": [
      "node_modules/**"
    ]
}